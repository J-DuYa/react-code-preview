{
  "name": "@uiw/react-code-preview",
  "version": "1.11.1",
  "description": "Code edit preview for React.",
  "author": "Kenny Wang <wowohoo@qq.com>",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "typings": "lib/cjs/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "released": "npm run build && npm run doc",
    "doc": "ENTRYDIR=website kkt build",
    "doc:dev": "ENTRYDIR=website kkt start",
    "build": "npm run ts:build && npm run types:esm && npm run types:cjs && npm run css:build",
    "types:build": "tsbb types --sourceRoot src --target ESNEXT",
    "types:watch": "npm run types:esm -- --watch & npm run types:cjs -- --watch",
    "types:esm": "npm run types:build -- --outDir ../lib/esm",
    "types:cjs": "npm run types:build -- --outDir ../lib/cjs",
    "css:build": "compile-less -d src -o lib/esm",
    "ts:watch": "tsbb watch --env-name esm:dev --env-name cjs --target react",
    "ts:build": "tsbb build --target react"
  },
  "homepage": "https://uiwjs.github.io/react-code-preview",
  "repository": {
    "type": "git",
    "url": "https://github.com/uiwjs/react-code-preview"
  },
  "files": [
    "src",
    "lib"
  ],
  "keywords": [],
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@kkt/loader-less": "^1.0.3",
    "@types/classnames": "^2.2.7",
    "@types/codemirror": "0.0.76",
    "@types/highlight.js": "^9.12.3",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-test-renderer": "^16.8.1",
    "@types/react-transition-group": "^4.2.0",
    "@uiw/reset.css": "^1.0.3",
    "compile-less-cli": "^1.1.5",
    "highlight.js": "^9.15.9",
    "kkt": "^5.0.0-alpha.11",
    "raw-loader": "^3.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-markdown": "^4.1.0",
    "react-test-renderer": "^16.8.6",
    "tsbb": "^1.3.5"
  },
  "dependencies": {
    "@babel/plugin-transform-classes": "^7.5.5",
    "@babel/runtime": "^7.7.2",
    "@babel/standalone": "^7.7.3",
    "@uiw/copy-to-clipboard": "^1.0.10",
    "@uiw/react-codemirror": "^2.0.0",
    "babel-plugin-transform-remove-imports": "^1.2.0",
    "classnames": "^2.2.6",
    "uiw": "^3.9.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
